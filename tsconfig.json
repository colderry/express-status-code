		"module": "commonjs",
		"target": "es2017",
		"strict": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"noUnusedLocals": true,
		"isolatedModules": true,
		"declaration": true,
		"sourceMap": true,
		"incremental": true,
